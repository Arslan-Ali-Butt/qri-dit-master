- page_title = 'QRID Photos'
= tenant_page_title page_title

.row
  .col-lg-8
    %h1= page_title
  .col-lg-4
    = link_to 'Back to Previous', tenant_qrids_url, class: 'btn btn-primary pull-right', role: 'button'
.row
  .col-lg-12
    .panel.panel-default
      .panel-heading
        %h3.panel-title= "#{@parent.name} Photos"
      .panel-body
        = render 'photos'

= render 'upload_progress_template'