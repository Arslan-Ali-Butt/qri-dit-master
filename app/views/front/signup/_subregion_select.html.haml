<div id='signup_tenants_state_code_wrapper'>
  <% parent_region ||= @billingaddress_country %>
  <% country = Carmen::Country.coded(parent_region) %>

  <% if country.nil? %>
    <em>Please select a country above</em>
  <% elsif country.subregions? %>
    <%= subregion_select(:signup, :state_code, parent_region) %>
  <% else %>
    <%= text_field(:signup, :state_code) %>
  <% end %>
</div>
